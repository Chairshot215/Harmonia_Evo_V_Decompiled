    <string name="tutorial_app_name">About Face Unlock</string>
    <string name="tutorial_header">Look at your phone to unlock it.</string>
    <string name="tutorial_info_text">"Keep these things in mind:

 "<li><ul>"Face Unlock is less secure than a pattern, PIN, or password.

 "<li>Someone who looks similar to you could unlock your phone.</li>"

 "<li>The data used to identify your face is kept private on the phone.</li>"

 "</ul></li></string>
    <string name="tutorial_continue_label">Set it up</string>
    <string name="intro_header">For best results:</string>
    <string name="intro_info_text">" "<li><ul>"Find an indoor spot—not too bright or too dim.

 "<li>Hold the phone at eye level.</li>"

 "</ul></li></string>
    <string name="intro_continue_label">Continue</string>
    <string name="endscreen_header">"You're all done!"</string>
    <string name="endscreen_top_info_text">To unlock your phone, just look at it.</string>
    <string name="endscreen_bottom_info_text">Tip: Improve face matching by having your face captured in different conditions.</string>
    <string name="endscreen_continue_label">OK</string>
