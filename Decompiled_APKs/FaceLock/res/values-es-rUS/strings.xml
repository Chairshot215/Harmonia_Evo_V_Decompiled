    <string name="tutorial_app_name">Acerca del desbloqueo facial</string>
    <string name="tutorial_header">Mira tu dispositivo para desbloquearlo.</string>
    <string name="tutorial_info_text">"Ten en cuenta lo siguiente:

 "<li><ul>"La opción de desbloqueo facial es menos segura que un patrón de desbloqueo, un PIN o una contraseña.

 "<li>Una persona con rasgos parecidos a los tuyos podría desbloquear el dispositivo.</li>"

 "<li>Los datos utilizados para identificar tu rostro se almacenan de forma privada en el dispositivo.</li>"

 "</ul></li></string>
    <string name="tutorial_continue_label">Configurar</string>
    <string name="intro_header">Para obtener mejores resultados:</string>
    <string name="intro_info_text">" "<li><ul>"Busca un espacio interior con luz adecuada, que no sea demasiado intensa ni demasiado tenue.

 "<li>Sitúa el dispositivo a la altura de los ojos.</li>"

 "</ul></li></string>
    <string name="intro_continue_label">Continuar</string>
    <string name="endscreen_header">Ya has terminado.</string>
    <string name="endscreen_top_info_text">Para desbloquear el dispositivo, solo míralo.</string>
    <string name="endscreen_bottom_info_text">Sugerencia: Mejora el reconocimiento facial al realizar tomas de tu rostro en diferentes condiciones.</string>
    <string name="endscreen_continue_label">Aceptar</string>
    <string name="addtosetup_app_name">Mejorar el reconocimiento facial</string>
    <string name="addtosetup_header">Realiza tomas de tu rostro en diferentes codiciones.</string>
    <string name="addtosetup_info_text">"Ejemplos:

 "<li><ul>"Con distinta iluminación,

 "<li>con o sin anteojos recetados,</li>"

 "<li>con barba o afeitado.</li>"

 "</ul></li>"
Sitúa el dispositivo a la altura de los ojos."</string>
