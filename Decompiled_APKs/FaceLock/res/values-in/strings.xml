    <string name="endscreen_bottom_info_text">Kiat: Tingkatkan kecocokan wajah dengan memotret wajah Anda dalam kondisi yang berbeda-beda.</string>
    <string name="endscreen_continue_label">OK</string>
    <string name="addtosetup_app_name">Menyempurnakan pencocokan wajah</string>
    <string name="addtosetup_header">Potret wajah Anda dalam kondisi yang berbeda-beda.</string>
    <string name="addtosetup_info_text">"Contoh:

